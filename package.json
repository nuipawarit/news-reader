{
  "name": "news-reader",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.8",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "fontsource-roboto": "^3.0.3",
    "import-sort-style-react": "^5.1.0",
    "jest-enzyme": "^7.1.2",
    "lodash": "^4.17.20",
    "prettier": "^2.2.0",
    "prettier-plugin-import-sort": "0.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-router-dom": "^5.2.0",
    "react-router-last-location": "^2.0.1",
    "react-router-scroll-memory": "^2.0.7",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "typescript": "4.0.5",
    "web-vitals": "^0.2.4"
  },
  "resolutions": {
    "react-helmet/react-side-effect": "2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write src/**/*.{js,json,jsx,scss,ts,tsx}",
    "format:check": "prettier --check src/**/*.{js,json,jsx,ts,tsx}",
    "format:debug": "prettier --debug-check src/**/*.{js,json,jsx,ts,tsx}",
    "lint:script": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:script:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript",
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "react",
      "parser": "typescript"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
